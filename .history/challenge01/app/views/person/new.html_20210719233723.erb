<h3>Create a Dog</h3>
<%= form_with model: @person do |form| %>
  <%= form.text_field :name, placeholder: "name" %>
  <%= form.text_field :nickname, placeholder: "nickname" %>
  <%= form.text_field :last_name, placeholder: "last name" %>
  <%= form.number :age, placeholder: "age" %>
  <%= form.text_field :peso, placeholder: "height" %>
  <%= form.text_field :altura, placeholder: "name" %>
  <%= form.text_field :name, placeholder: "name" %>
  <%= form.submit %>
<% end %>

